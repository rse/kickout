    "dependencies" : {
        "yargs":                  "17.1.0",
        "update-notifier":        "5.1.0",
        "escape-string-regexp":   "4.0.0",
        "bluebird":               "3.7.2",
        "execa":                  "5.1.1",
        "get-stream":             "6.0.1",
        "semver":                 "7.3.5",
        "chalk":                  "4.1.2",
